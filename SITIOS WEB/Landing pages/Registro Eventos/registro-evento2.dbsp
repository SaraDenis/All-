<body>
    <#include doc="navegacion_superior.html"/>

    <#include doc="header.html"/> 

	<script>
	var campos_forma = new Array ("nombre","correoelectronico","telefono");
	function ValidateFormX(forma) {
	if (jQuery('[name="nombre"]').val() != "" && jQuery('[name="telefono"]').val() != "" && !isNaN(jQuery('[name="telefono"]').val()) && jQuery('[name="correoelectronico"]').val() != "")
	{
	  return true
	}
	else {
		  if (jQuery('[name="nombre"]').val() == ""){
		  	jQuery('[name="nombre"]').addClass("DatoMal");
		  }
		  if (jQuery('[name="correoelectronico"]').val() == ""){
		  	jQuery('[name="correoelectronico"]').addClass("DatoMal");
		  }
		  if (jQuery('[name="telefono"]').val() == "" || isNaN(jQuery('[name="telefono"]').val())){
		  	jQuery('[name="telefono"]').addClass("DatoMal");
		  }
	  return false
		}
	} 
	</script>

	<style>
		label {font-size: 12px; text-transform: uppercase; font-weight: 700;}
		label span {color:#F37922;}
		#transparentBox {height:100px;}
		#btn-registro {padding:10px 35px; background-color:#F37922; font-weight: bold;}
		#btn-registro:hover {background-color:#D25807;}
		@media (max-width: 767px) {#transparentBox {height:50px;}}
	</style>

	<div id="transparentBox"></div>
	<div class="container">
		<div class="col-md-6 col-md-offset-3">
			<h1>Registro</h1>
			<p></p>
			<form name="calificacion" class="well" method="post" onsubmit="return ValidateFormX(this)" action="http://op2020.salesup.com.mx/evento-registro.dbsp">
				<input type="hidden" name="idevento" value="<#idevento/>"/>
				<input type="hidden" name="refpage" value="/"/>
				<div class="form-group">
					<label>Título</label>
					<select name="titulo" class="form-control">
						<option value="0" selected="" disabled="">Seleccionar...</option>
						<option>Sr.</option>
					    <option>Sra.</option>
					    <option>Srita.</option>
					    <option>Lic.</option>
					    <option>Ing.</option>
					    <option>Arq.</option>
					    <option>Act.</option>
					    <option>Mtro.</option>
					    <option>Dr.</option>
					</select>
				</div>
				<div class="form-group">
					<label>Nombre <span>*</span></label>
					<input type="text" class="form-control esObligatorio" name="nombre" maxlength=256/>
				</div>
				<div class="form-group">
					<label>Teléfono (con lada) <span>*</span></label>
					<input type="tel" name="telefono" maxlength=128 class="form-control esObligatorio" onkeypress="javscript:return ValidaEntero(event)"/>
				</div>
				<div class="form-group">
					<label>Celular</label>
					<input type="text" class="form-control" name="celular" maxlength=128 onkeypress="javscript:return ValidaEntero(event)"/>
				</div>
				<div class="form-group">
					<label>Correo electrónico <span>*</span></label>
					<input type="email" class="form-control" name="correoelectronico" class="esObligatorio" maxlength=128/>
				</div>
				<div class="form-group">
					<label>Compañía</label>
					<input type="text" class="form-control" name="razonsocial" maxlength=128/>
				</div>
				<div class="form-group">
					<label>Giro</label>
					<input type="text" class="form-control" name="giro" maxlength=128/>
				</div>
				<div class="form-group">
					<label>Sitio web</label>
					<input type="text" class="form-control" name="sitioweb" maxlength=1024/>
				</div>
				<div class="form-group">
					<label>Número de empleados</label>
					<input type="text" class="form-control" name="numeroempleados" maxlength=6 onkeypress="javscript:return ValidaEntero(event)"/>
				</div>
				<div class="form-group">
					<label>Número de vendedores</label>
					<input type="text" class="form-control" name="numerovendedores" maxlength=6 onkeypress="javscript:return ValidaEntero(event)"/>
				</div>
				<div class="form-group">
					<label>¿Qué te gustaría haber logrado al finalizar el curso?</label>
					<textarea name="comentarios" class="form-control" rows="4"></textarea>
				</div>
				<div class="form-group">
					<button id="btn-registro" type="submit">Registrar al evento</button>
				</div>
			</form>
		</div>
	</div> 
</body>
